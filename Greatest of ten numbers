int main(int argc,char *argv[])
{
    int i,j,temp,c[10],largest;
    j=0;
    for(i=1;i<argc;i++)
    {
        temp=atoi(argv[i]);
        c[j]=temp;
        j++;
    }
    
    largest=c[0];
    for(i=0;i<10;i++)
    {
        if(c[i]>largest)
        {
            largest=c[i];
        }
        
    }
   
printf("greatest no. of ten no.:%d",largest);
    return 0;
}
